{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"db:push": {
			"inputs": ["prisma/schema.prisma"],
			"cache": false
		},
		"db:push:force": {
			"inputs": ["prisma/schema.prisma"],
			"cache": false
		},
		"db:generate": {
			"inputs": ["prisma/schema.prisma"],
			"cache": false
		},
		"db:seed": {
			"inputs": ["prisma/seed.ts"],
			"cache": false
		},
		"dev": {
			"dependsOn": ["^db:generate"],
			"cache": false
		},
		"build": {
			"dependsOn": ["^build", "^db:generate"],
			"outputs": [".next/**", "dist/**"]
		},
		"lint": {
			"outputs": []
		},
		"clean": {
			"cache": false
		},
		"//#clean": {
			"cache": false
		},
		"type-check": {
			"dependsOn": ["^db:generate"],
			"cache": false
		}
	},
	"globalDependencies": [".env"]
}
