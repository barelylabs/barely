{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": ["**/.env"],
  "pipeline": {
    "dev": {
      "persistent": true,
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "next-env.d.ts",
        ".output/**",
        ".vercel/output/**"
      ]
    },
    "format": {
      "outputs": ["node_modules/.cache/.prettiercache"],
      "outputMode": "new-only"
    },
    "lint": {
      "outputs": ["node_modules/.cache/.eslintcache"]
    },
    "typecheck": {
      "outputs": ["node_modules/.cache/tsbuildinfo.json"]
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    }
  },
  "globalEnv": [
    "NEXT_PUBLIC_APP_DEV_PORT",
    "NEXT_PUBLIC_LINK_DEV_PORT",
    "NEXT_PUBLIC_WWW_DEV_PORT",
    "NEXT_PUBLIC_CLOUDINARY_API_KEY",
    "NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME",
    "NEXT_PUBLIC_PUSHER_APP_CLUSTER",
    "NEXT_PUBLIC_PUSHER_APP_ID",
    "NEXT_PUBLIC_PUSHER_APP_KEY",
    "NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY",
    "NEXT_PUBLIC_LINK_ROOT_DOMAIN",
    "NEXT_PUBLIC_SHORT_LINK_ROOT_DOMAIN",
    "NEXT_PUBLIC_TRANSPARENT_LINK_ROOT_DOMAIN",
    "NEXT_PUBLIC_VERCEL_DEPLOYMENT_SUFFIX",

    "AUTH_URL",
    "AUTH_SECRET",
    "BOT_SPOTIFY_ACCOUNT_ID",
    "BOT_THREADS_API_KEY",
    "CLOUDINARY_API_SECRET",
    "DATABASE_NAME",
    "DATABASE_READ_URL",
    "DATABASE_READ_POOL_URL",
    "DATABASE_WRITE_URL",
    "DATABASE_WRITE_POOL_URL",
    "DATABASE_URL",
    "DISCORD_CLIENT_ID",
    "DISCORD_CLIENT_SECRET",
    "GANDI_API_KEY",
    "LOCALHOST_IP",
    "NAMESILO_API_KEY",
    "NEXTAUTH_SECRET",
    "NEXTAUTH_URL",
    "NEXTAUTH_URL_INTERNAL",
    "NODE_ENV",
    "OPENAI_API_KEY",
    "OPENAI_ORG_ID",
    "POSTMARK_SERVER_API_TOKEN",
    "PUSHER_APP_SECRET",
    "RATE_LIMIT_RECORD_LINK_CLICK",
    "RESEND_API_KEY",
    "SCREENING_PHONE_NUMBER",
    "SENDGRID_API_KEY",
    "SPOTIFY_CLIENT_ID",
    "SPOTIFY_CLIENT_SECRET",
    "STRIPE_SECRET_KEY",
    "STRIPE_WEBHOOK_SECRET",
    "TINYBIRD_API_KEY",
    "TWILIO_ACCOUNT_SID",
    "TWILIO_AUTH_TOKEN",
    "TWILIO_PHONE_NUMBER",
    "UPSTASH_REDIS_REST_URL",
    "UPSTASH_REDIS_REST_TOKEN",
    "VERCEL_ENV",
    "VERCEL_LINK_PROJECT_ID",
    "VERCEL_TEAM_ID",
    "VERCEL_TOKEN",
    "VERCEL_URL"
  ]
}
