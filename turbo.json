{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"db:push": {
			"inputs": ["prisma/schema.prisma"],
			"cache": false
		},
		"db:push:force": {
			"inputs": ["prisma/schema.prisma"],
			"cache": false
		},
		"db:generate": {
			"inputs": ["prisma/schema.prisma"],
			"cache": false
		},
		"db:seed": {
			"inputs": ["prisma/seed.ts"],
			"cache": false
		},
		"dev": {
			"persistent": true,
			"cache": false
		},
		"build": {
			"dependsOn": ["^build", "^db:generate"],
			"outputs": [".next/**", "dist/**"]
		},
		"lint": {},
		"clean": {
			"cache": false
		},
		"//#clean": {
			"cache": false
		},
		"type-check": {
			"dependsOn": ["^db:generate"],
			"cache": false
		}
	},
	"globalEnv": [
		"NEXT_PUBLIC_APP_DEV_PORT",
		"NEXT_PUBLIC_WEB_DEV_PORT",
		"NEXT_PUBLIC_LINK_DEV_PORT",
		"NEXT_PUBLIC_PUSHER_APP_ID",
		"NEXT_PUBLIC_PUSHER_APP_KEY",
		"NEXT_PUBLIC_PUSHER_APP_CLUSTER",
		"NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY",

		"VERCEL_ENV",
		"VERCEL_URL",
		"BOT_SPOTIFY_ACCOUNT_ID",

		"DATABASE_URL",
		"NEXTAUTH_SECRET",
		"NEXTAUTH_URL",
		"NEXTAUTH_URL_INTERNAL",
		"OPENAI_API_KEY",
		"OPENAI_ORG_ID",
		"POSTMARK_SERVER_API_TOKEN",
		"PUSHER_APP_SECRET",
		"SCREENING_PHONE_NUMBER",
		"SENDGRID_API_KEY",
		"SPOTIFY_CLIENT_ID",
		"SPOTIFY_CLIENT_SECRET",
		"STRIPE_SECRET_KEY",
		"STRIPE_WEBHOOK_SECRET",
		"TWILIO_ACCOUNT_SID",
		"TWILIO_AUTH_TOKEN",
		"TWILIO_PHONE_NUMBER"
	]
}
