{
	"name": "@barely/app",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "pnpm with-env next build",
		"build:preview": "pnpm with-env vercel build",
		"build:production": "pnpm with-env vercel build --prod",
		"clean": "git clean -xdf .next .turbo node_modules",
		"dev": "pnpm with-env next dev --experimental-https --port=3000",
		"dev:pre": "git clean -xdf .next",
		"format": "prettier --check . --ignore-path ../../.gitignore",
		"lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
		"start": "pnpm with-env next start",
		"typecheck": "tsc --noEmit",
		"with-env": "dotenv -e ../../.env --"
	},
	"dependencies": {
		"@barely/email": "workspace:*",
		"@barely/lib": "workspace:*",
		"@barely/ui": "workspace:*",
		"@formkit/auto-animate": "1.0.0-beta.6",
		"@mdxeditor/editor": "^2.18.0",
		"@t3-oss/env-nextjs": "^0.10.1",
		"@tailwindcss/forms": "^0.5.6",
		"@tailwindcss/nesting": "0.0.0-insiders.565cd3e",
		"@tanstack/query-sync-storage-persister": "^5.17.1",
		"@tanstack/react-query": "^5.25.0",
		"@tanstack/react-query-devtools": "^5.25.0",
		"@tanstack/react-query-next-experimental": "5.25.0",
		"@tanstack/react-query-persist-client": "^5.25.0",
		"@trpc/client": "11.0.0-rc.334",
		"@trpc/next": "11.0.0-rc.334",
		"@trpc/server": "11.0.0-rc.334",
		"bufferutil": "4.0.8",
		"dotenv-cli": "^7.4.1",
		"fast-deep-equal": "^3.1.3",
		"jiti": "^1.21.0",
		"jotai": "^2.6.1",
		"next": "^14.2.3",
		"object-path-immutable": "^4.1.2",
		"perfect-debounce": "^1.0.0",
		"pusher-js": "^8.3.0",
		"react": "18.2.0",
		"react-aria": "^3.32.1",
		"react-aria-components": "^1.1.1",
		"react-dom": "18.2.0",
		"react-error-boundary": "^4.0.10",
		"react-hook-form": "^7.45.4",
		"remeda": "^1.24.0",
		"sharp": "^0.33.2",
		"superjson": "^2.2.1",
		"tailwindcss-animate": "^1.0.7",
		"trpc-openapi": "^1.2.0",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@barely/eslint-config": "workspace:*",
		"@barely/prettier-config": "workspace:*",
		"@barely/tailwind-config": "workspace:*",
		"@barely/tsconfig": "workspace:*",
		"@swc-jotai/debug-label": "^0.0.10",
		"@swc-jotai/react-refresh": "^0.0.8",
		"@tailwindcss/typography": "^0.5.9",
		"@types/node": "^20.10.6",
		"@types/react": "^18.2.25",
		"@types/react-dom": "^18.2.18",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.57.0",
		"eslint-config-next": "^14.0.4",
		"postcss": "^8.4.32",
		"prettier": "^3.2.5",
		"tailwindcss": "^3.4.0",
		"typescript": "^5.4.3"
	},
	"eslintConfig": {
		"root": true,
		"extends": [
			"@barely/eslint-config/base",
			"@barely/eslint-config/nextjs",
			"@barely/eslint-config/react"
		]
	},
	"prettier": "@barely/prettier-config"
}
