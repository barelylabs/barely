{
  "name": "@barely/lib",
  "version": "0.1.0",
  "private": true,
  "files": [
    "api/**/*",
    "auth/**/*",
    "hooks/**/*",
    "meta/**/*",
    "schema/**/*",
    "sms/**/*",
    "spotify/**/*",
    "utils/**/*",
    "env"
  ],
  "scripts": {
    "clean": "git clean -xdf .turbo node_modules",
    "db:check": "pnpm with-env drizzle-kit check:pg --config=server/db/drizzle.config.ts",
    "db:drop": "pnpm with-env drizzle-kit drop --config=server/db/drizzle.config.ts",
    "db:pull": "pnpm with-env drizzle-kit introspect:pg --config=server/db/drizzle.config.ts",
    "db:push": "pnpm with-env drizzle-kit push:pg --config=server/db/drizzle.config.ts",
    "db:generate": "pnpm with-env drizzle-kit generate:pg --config=server/db/drizzle.config.ts",
    "dev": "pnpm with-env drizzle-kit studio --port 5555 --config=server/db/drizzle.config.ts --verbose",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@auth/core": "^0.19.0",
    "@barely/email": "workspace:^0.1.0",
    "@barely/env": "workspace:^0.1.0",
    "@barely/toast": "workspace:^0.1.0",
    "@hookform/resolvers": "^3.2.0",
    "@langchain/core": "^0.1.17",
    "@langchain/openai": "^0.0.12",
    "@neondatabase/serverless": "^0.5.7",
    "@noble/hashes": "1.3.0",
    "@paralleldrive/cuid2": "^2.2.1",
    "@sindresorhus/slugify": "^2.2.1",
    "@t3-oss/env-nextjs": "^0.7.3",
    "@trpc/react-query": "next",
    "@trpc/server": "next",
    "@upstash/ratelimit": "^0.4.4",
    "@upstash/redis": "^1.24.3",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "class-variance-authority": "^0.7.0",
    "cloudinary": "^1.41.0",
    "clsx": "^2.0.0",
    "devalue": "^4.3.2",
    "drizzle-orm": "^0.29.3",
    "drizzle-zod": "^0.5.1",
    "fast-deep-equal": "^3.1.3",
    "fast-xml-parser": "^4.3.2",
    "jotai": "^2.6.1",
    "langchain": "^0.1.5",
    "lexorank": "^1.0.5",
    "libphonenumber-js": "^1.10.37",
    "next": "^14.1.0",
    "next-auth": "5.0.0-beta.4",
    "node-html-parser": "^6.1.11",
    "postgres": "^3.3.5",
    "punycode": "^2.3.1",
    "pusher-http-edge": "^0.4.0",
    "react-dom-confetti": "^0.2.0",
    "remeda": "^1.24.0",
    "sha.js": "^2.4.11",
    "stripe": "^11.18.0",
    "superjson": "^1.13.1",
    "tailwind-merge": "^1.14.0",
    "trpc-openapi": "^1.2.0",
    "twilio": "^3.84.1",
    "xml2js": "^0.6.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@barely/eslint-config": "workspace:^0.1.0",
    "@barely/prettier-config": "workspace:^0.1.0",
    "@barely/tailwind-config": "workspace:^0.1.0",
    "@barely/tsconfig": "workspace:^0.1.0",
    "@polyscale/serverless-js": "^1.4.0",
    "@types/html-escaper": "^3.0.1",
    "@types/node": "^20.10.6",
    "@types/punycode": "^2.1.2",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.18",
    "@types/sha.js": "^2.4.1",
    "@types/xml2js": "^0.4.14",
    "dotenv-cli": "^7.2.1",
    "drizzle-kit": "^0.20.14",
    "encoding": "^0.1.13",
    "eslint": "^8.56.0",
    "pg": "^8.11.1",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.4",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.4"
  },
  "eslintConfig": {
    "extends": [
      "@barely/eslint-config/base"
