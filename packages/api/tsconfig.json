{
	"extends": "@barely/tsconfig/internal-package.json",
	"compilerOptions": {
		"lib": ["ES2022", "DOM"],
		"jsx": "preserve",
		"rootDir": ".",
		"outDir": "dist",
		"plugins": [
			{
				"name": "trpc-navigation-plugin",
				"router": {
					"filePath": "./src/app/app.router.ts",
					"variableName": "appRouter"
				}
			}
		]
	},
	"include": ["*.ts", "src"],
	"exclude": ["node_modules", "coverage"]
}
