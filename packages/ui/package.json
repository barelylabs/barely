		"tsup": "^5.10.1",
		"typescript": "^4.9.3"
	},
	"dependencies": {
		"@headlessui/react": "^1.7.4",
		"@radix-ui/react-tooltip": "^1.0.2",
		"class-variance-authority": "^0.3.0",
		"clazzx": "^0.0.2",
