{
	"name": "@barely/tb",
	"private": true,
	"version": "0.1.0",
	"type": "module",
	"exports": {
		".": "./src/index.ts",
		"./constants": "./src/constants/index.ts",
		"./ingest": "./src/ingest/index.ts",
		"./schema": "./src/schema/index.ts",
		"./query": "./src/query/index.ts"
	},
	"license": "MIT",
	"scripts": {
		"build": "tsc",
		"clean": "git clean -xdf .cache .turbo dist node_modules",
		"dev": "concurrently \"tb dev\" \"tsc\"",
		"format": "prettier --check . --ignore-path ../../.gitignore",
		"lint": "eslint",
		"typecheck": "tsc --noEmit --emitDeclarationOnly false",
		"tb:login": "cd tinybird && tb login",
		"tb:start": "cd tinybird && tb local start",
		"tb:build": "cd tinybird && tb build",
		"tb:test": "cd tinybird && tb test run",
		"tb:check": "cd tinybird && tb deploy --check",
		"tb:mock": "cd tinybird && tb mock",
		"tb:pull-data": "cd tinybird && ./scripts/tb-dev-pull.sh"
	},
	"dependencies": {
		"@barely/const": "workspace:*",
		"@barely/db": "workspace:*",
		"@barely/utils": "workspace:*",
		"@barely/validators": "workspace:*",
		"@t3-oss/env-nextjs": "catalog:",
		"zod": "catalog:"
	},
	"devDependencies": {
		"@barely/eslint-config": "workspace:*",
		"@barely/prettier-config": "workspace:*",
		"@barely/tsconfig": "workspace:*",
		"@types/node": "catalog:",
		"eslint": "catalog:",
		"prettier": "catalog:",
		"typescript": "catalog:"
	},
	"prettier": "@barely/prettier-config"
}
