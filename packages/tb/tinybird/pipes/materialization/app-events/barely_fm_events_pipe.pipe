NODE mv
SQL >

    SELECT 
      timestamp,
      type,
      workspaceId,
      assetId,
      platform,
  
      city,
      region,
      country,
      device,
      browser,
      os,
      referer,
  
      fanId,
      sessionId,
      sessionReferer,
      sessionEmailTemplateId,
      sessionEmailBroadcastId,
      sessionLandingPageId,
      sessionFlowActionId,
      sessionMetaCampaignId,
      sessionMetaAdSetId,
      sessionMetaAdId,
      sessionMetaPlacement
  
    FROM barely_events
    WHERE assetId LIKE 'fm_%'

TYPE materialized
DATASOURCE barely_fm_events_mv


