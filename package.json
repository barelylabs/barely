{
	"name": "create-t3-turbo",
	"private": true,
	"packageManager": "pnpm@7.24.2",
	"workspaces": [
		"apps/*",
		"config/*",
		"lib/*",
		"packages/*"
	],
	"scripts": {
		"build:libs": "turbo build:libs",
		"build": "turbo build",
		"clean": "rm -rf node_modules && rm -rf dist",
		"clean:workspaces": "turbo clean",
		"clean:all": " turbo clean && rm -rf node_modules",
		"db:generate": "turbo db:generate",
		"db:push": "turbo db:push",
		"db:push:force": "turbo db:push:force ",
		"db:seed": "turbo db:seed",
		"dev": "turbo dev --parallel",
		"format": "prettier --write \"**/*.{ts,tsx,md}\"",
		"preinstallz": "mkdir ./libs/prisma-generator-zod/dist && touch ./libs/prisma-generator-zod/dist/cli.js",
		"lint": "turbo lint && manypkg check",
		"type-check": "turbo type-check"
	},
	"dependencies": {
		"@manypkg/cli": "^0.19.2",
		"@typescript-eslint/eslint-plugin": "^5.43.0",
		"@typescript-eslint/parser": "^5.43.0",
		"eslint": "^8.28.0",
		"prettier": "^2.7.1",
		"prettier-plugin-prisma": "^4.8.0",
		"prettier-plugin-tailwindcss": "^0.1.13",
		"turbo": "^1.6.3",
		"typescript": "^4.9.3"
	}
}
